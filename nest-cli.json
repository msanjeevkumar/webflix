{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/webflix/src",
  "monorepo": true,
  "root": "apps/webflix",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/webflix/tsconfig.app.json"
  },
  "projects": {
    "webflix": {
      "type": "application",
      "root": "apps/webflix",
      "entryFile": "main",
      "sourceRoot": "apps/webflix/src",
      "compilerOptions": {
        "tsConfigPath": "apps/webflix/tsconfig.app.json"
      }
    },
    "movies": {
      "type": "application",
      "root": "apps/movies",
      "entryFile": "main",
      "sourceRoot": "apps/movies/src",
      "compilerOptions": {
        "tsConfigPath": "apps/movies/tsconfig.app.json"
      }
    },
    "genres": {
      "type": "application",
      "root": "apps/genres",
      "entryFile": "main",
      "sourceRoot": "apps/genres/src",
      "compilerOptions": {
        "tsConfigPath": "apps/genres/tsconfig.app.json"
      }
    }
  }
}