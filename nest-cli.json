{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/webflix/src",
  "monorepo": true,
  "root": "apps/webflix",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/webflix/tsconfig.app.json"
  },
  "projects": {
    "webflix": {
      "type": "application",
      "root": "apps/webflix",
      "entryFile": "main",
      "sourceRoot": "apps/webflix/src",
      "compilerOptions": {
        "tsConfigPath": "apps/webflix/tsconfig.app.json"
      }
    },
    "movies": {
      "type": "application",
      "root": "apps/movies",
      "entryFile": "main",
      "sourceRoot": "apps/movies/src",
      "compilerOptions": {
        "tsConfigPath": "apps/movies/tsconfig.app.json"
      }
    },
    "genres": {
      "type": "application",
      "root": "apps/genres",
      "entryFile": "main",
      "sourceRoot": "apps/genres/src",
      "compilerOptions": {
        "tsConfigPath": "apps/genres/tsconfig.app.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}
